#include <iostream>
using namespace std;

struct number {
    int num;
};

void swap(number &a, number &b) {  
    a.num = b.num;
    b.num = c;
    cout << "After swapping:" << endl;
    cout << "Number 1 = " << a.num << endl;
    cout << "Number 2 = " << b.num << endl;
}

int main() {
    cout << "NAME: SABRINA MANAHIL WASEEM" << endl
         << "ROLL NO: FESE-23006" << endl << "\n";

    number n1, n2;
    cout << "Enter number 1: ";
    cin >> n1.num;
    cout << "Enter number 2: ";
    cin >> n2.num;

    swap(n1, n2);  

    cout << "\nAfter swap in main:" << endl;
    cout << "Number 1 = " << n1.num << endl;
    cout << "Number 2 = " << n2.num << endl;

    return 0;
}
